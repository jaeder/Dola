        {
            "name": "Jaeder Doo",
            "email": "jaederl@outlook.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.3.0|~5.4.0",
        "intervention/image": "^2.3",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.0",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.35"
    },
    "require-dev": {
