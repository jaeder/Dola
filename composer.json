{
    "name": "dfz/dola",
    "description": "A Laravel  Package based on voyager",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "homepage": "https://github.com/jaeder/Dola",
    "support": {
        "issues": "",
        "source": "https://github.com/jaeder/Dola"
